        <span class="na">onChange</span><span class="p">=</span><span class="si">{</span><span class="nx">handleState</span><span class="si">}</span>
      <span class="p">/&gt;</span> <span class="p">&lt;</span><span class="err">-</span> <span class="err">í•­ì‹œ</span> <span class="err">ë‹¨ì¼</span> <span class="err">íƒœê·¸ëŠ”</span> <span class="err">ë‹«ì•„ì¤˜ì•¼í•œë‹¤!</span>
      <span class="err">&lt;</span><span class="na">button</span> <span class="na">onClick</span><span class="p">=</span><span class="si">{</span><span class="nx">handleAddStateList</span><span class="si">}</span><span class="p">&gt;</span>ì¶”ê°€<span class="p">&lt;/</span><span class="nt">button</span><span class="p">&gt;</span>
    <span class="p">&lt;/</span><span class="nt">div</span><span class="p">&gt;</span>
  <span class="p">);</span>
<span class="p">}</span>

